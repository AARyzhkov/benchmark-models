%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%               Pincells
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

pin pi			% Inner fuel pin
    void    0.100
    fueli   0.450
    void    0.465
    Ti1515  0.525
    lead

pin po			% Outer fuel pin
    void    0.100
    fuelo   0.450
    void    0.465
    Ti1515  0.525
    lead

pin pl			% Plenum pin
    void    0.4
    pln     0.465
    Ti1515  0.525
    lead

pin tg		 % Top plug "pin"
    tplg   0.465
    Ti1515 0.525
    lead

pin bg		 % Bottom plug "pin"
    bplg   0.465
    Ti1515 0.525
    lead

pin in		 % Top insulator "pin"
    ins    0.465
    Ti1515 0.525
    lead

pin dp		 % Dummy "pin"
    ZrYO   0.465
    Ti1515 0.525
    lead

pin sp		 % Spring "pin"
    spr    0.465
    Ti1515 0.525
    lead

pin s1		 % Safety pin
    safe   1.45
    void   1.49
    Ti1515 1.55
    lead

pin fp		 % Control pin follower 
    foll   1.49
    fmst91 1.55
    lead

pin l			
    lead
    
pin L			
    lead

pin B		
    barrel

pin V			
    void

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%               Surfaces
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

% Fuel rod (FR)
surf z_top      pz   170.0	      % Fuel pin upper limit
surf z_tplg_fr  pz   48.0	      % Fuel pin upper limit
surf z_sprn_fr  pz   43.0	      % Fuel pin upper limit
surf z_ins_u    pz   31.0         % Insulator upper limit
surf z_fuel_u   pz   30.0	      % Fuel active zone upper limit
surf z_fuel_b   pz  -30.0         % Fuel active zone lower limit
surf z_ins_b    pz  -31.0         % Insulator lower limit
surf z_plen     pz  -86.0         % Plenum fuel pin lower limit
surf z_bplg_fr  pz  -91.0         % Bottom plug
surf z_bot      pz  -170.0        % Bottom reflector

% Control rod (CR)
surf z_sprn_cr  pz    94.0	      % Fuel pin upper limit
surf z_frod_u   pz    34.0	      % Fuel pin upper limit
surf z_frod_b   pz   -34.0 	      % Insulator upper limit
surf z_crod_cr  pz  -102.0	      % Fuel active zone upper limit
surf z_bins_cr  pz  -103.0        % Fuel active zone lower limit
surf z_bplg_cr  pz  -108.0        % Bottom plug

% Safety rod (SR)
surf z_sprn_sr   pz   137.0	      % Fuel pin upper limit
surf z_tins_sr   pz   127.0	      % Fuel pin upper limit
surf z_srod_sr   pz   126.0 	  % Insulator upper limit
surf z_bplg_sr   pz    42.0	      % Fuel active zone upper limit
surf z_bref_sru  pz    37.0       % Upper reflector

surf sID hexyc 0. 0.   7.9
surf sOD hexyc 0. 0.   8.3
surf sC  hexxc 0. 0. 190.0

surf sSID1 cylz 0. 0. 7.35
surf sSOD1 cylz 0. 0. 8.35

surf sSID2 cylz 0. 0. 8.05
surf sSOD2 cylz 0. 0. 8.35

cell cInnerPin0  i fill bg           -z_plen
cell cInnerPin1  i fill pl z_plen    -z_ins_b
cell cInnerPin2  i fill in z_ins_b   -z_fuel_b
cell cInnerPin3  i fill pi z_fuel_b  -z_fuel_u
cell cInnerPin4  i fill in z_fuel_u  -z_ins_u
cell cInnerPin5  i fill sp z_ins_u   -z_sprn_fr
cell cInnerPin6  i fill tg z_sprn_fr 

cell cOuterPin0  o fill bg           -z_plen
cell cOuterPin1  o fill pl z_plen    -z_ins_b
cell cOuterPin2  o fill in z_ins_b   -z_fuel_b
cell cOuterPin3  o fill po z_fuel_b  -z_fuel_u
cell cOuterPin4  o fill in z_fuel_u  -z_ins_u
cell cOuterPin5  o fill sp z_ins_u   -z_sprn_fr
cell cOuterPin6  o fill tg z_sprn_fr 

cell cReflPin0  r fill bg           -z_plen
cell cReflPin1  r fill dp z_plen    -z_ins_u
cell cReflPin2  r fill sp z_ins_u   -z_sprn_fr
cell cReflPin3  r fill tg z_sprn_fr 

cell cCRPin0  c fill sp            -z_bins_cr
cell cCRPin1  c fill in  z_bins_cr -z_crod_cr
cell cCRPin2  c lead     z_crod_cr -z_frod_u
cell cCRPin3  c fill sp  z_frod_u 

cell cSRPin0  s fill bg            -z_bplg_sr
cell cSRPin1  s lead     z_bplg_sr -z_srod_sr
cell cSRPin2  s fill in  z_srod_sr -z_tins_sr
cell cSRPin3  s fill sp  z_tins_sr  

lat IL 2 0 0 15 15 1.386
    l l l l l l l l l l l l l l l
    l l l l l l l i i i i i i i l
    l l l l l l i i i i i i i i l
    l l l l l i i i i i i i i i l
    l l l l i i i i i i i i i i l
    l l l i i i i i i i i i i i l
    l l i i i i i i i i i i i i l
    l i i i i i i i i i i i i i l
    l i i i i i i i i i i i i l l
    l i i i i i i i i i i i l l l
